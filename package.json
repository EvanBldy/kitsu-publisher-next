{
  "name": "kitsu-publisher",
  "version": "0.1.0",
  "description": "Desktop publisher for Kitsu",
  "author": "CGWire <contact@cg-wire.com>",
  "repository": "https://github.com/cgwire/kitsu-publisher-next",
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "dev": "node scripts/watch.js",
    "prebuild": "cross-env MODE=production npm run build:js",
    "build": "electron-builder build --config electron-builder.config.js --dir",
    "prebuild:all": "cross-env MODE=production npm run build:js",
    "build:all": "electron-builder build --config electron-builder.config.js",
    "build:js": "node scripts/build.js",
    "pretest": "npm run build:js",
    "test": "node tests/app.spec.js",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .eslintignore --fix .",
    "format": "prettier . --ignore-path .gitignore --write"
  },
  "dependencies": {
    "apisauce": "2.1.2",
    "bulma": "0.3.1",
    "color-hash": "2.0.1",
    "color-string": "1.6.0",
    "fabric": "4.6.0",
    "marked": "3.0.4",
    "moment-timezone": "0.5.34",
    "sanitize-html": "2.5.3",
    "secure-ls": "1.2.6",
    "superagent": "6.1.0",
    "thenby": "1.3.4",
    "v-tooltip": "4.0.0-beta.2",
    "vue": "3.2.22",
    "vue-feather": "2.0.0-rc.1",
    "vue-i18n": "9.1.9",
    "vue-router": "4.0.12",
    "vue-slider-component": "3.2.15",
    "vue3-datepicker": "0.2.5",
    "vue3-lazyload": "0.2.2-beta",
    "vuex": "4.0.2",
    "vuex-persistedstate": "4.1.0",
    "vuex-router-sync": "6.0.0-rc.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "1.10.0",
    "browserslist": "4.18.1",
    "cross-env": "7.0.3",
    "electron": "16.0.1",
    "electron-builder": "22.14.5",
    "electron-devtools-installer": "3.2.0",
    "eslint": "8.3.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "8.1.1",
    "playwright": "1.16.3",
    "prettier": "2.4.1",
    "sass": "1.43.4",
    "sass-loader": "12.3.0",
    "vite": "2.6.14",
    "vite-plugin-eslint": "1.3.0"
  },
  "browserslist": [
    "Chrome 96"
  ],
  "engines": {
    "node": ">=16.11",
    "npm": ">=8.1"
  }
}
